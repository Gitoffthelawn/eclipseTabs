{
  "manifest_version": 2,
  "name": "Tab-folders",
  "version": "1.0",
  "description": "Let you manage your tabs using folders",
  "homepage_url": "https://universegame.de",
  "icons": {
    "48": "icons/logo-48.png"
  },
  "permissions": [
    "storage",
    "<all_urls>",
    "tabs",
    "contextMenus"
  ],
  "options_ui": {
    "page": "Settings.html"
  },
  "sidebar_action": {
    "default_title": "Tab-Folders",
    "default_panel": "sidebar/sidebar.html",
    "default_icon": "icons/logo-48.png"
  }
}